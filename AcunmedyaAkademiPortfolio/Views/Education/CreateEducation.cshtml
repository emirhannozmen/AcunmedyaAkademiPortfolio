
@{
    ViewBag.Title = "CreateEducation";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Eğitim Ekleme</h2>
<form method="post">

    <label>Bölüm</label>
    <input type="text" class="form-control" name="Title" style="border: groove" />
    <br />

    <label>Üniversite</label>
    <input type="text" class="form-control" name="SubTitle" style="border: groove" />
    <br />

    <label>Eğitim</label>
    <input type="text" class="form-control" name="Description" style="border: groove" />
    <br />

    <label>Dönem</label>
    <input type="text" class="form-control" name="Period" style="border: groove" />
    <br />

    <button class="btn btn-primary">Ekle</button>
</form>