
@{
    ViewBag.Title = "CreateAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Hakkımda Ekleme</h2>
<form method="post">

    <label>İsim</label>
    <input type="text" class="form-control" name="Title" style="border: groove" />
    <br />

    <label>Açıklama</label>
    <input type="text" class="form-control" name="Description" style="border: groove" />
    <br />

    <label>Yaş</label>
    <input type="text" class="form-control" name="Age" style="border: groove" />
    <br />

    <label>E-posta</label>
    <input type="text" class="form-control" name="Email" style="border: groove" />
    <br />

    <label>Doğum Tarihi</label>
    <input type="text" class="form-control" name="Birthday" style="border: groove" />
    <br />

    <label>Şehir</label>
    <input type="text" class="form-control" name="Location" style="border: groove" />
    <br />

    <label>Telefon</label>
    <input type="text" class="form-control" name="Phone" style="border: groove" />
    <br />

    <label>Web Sitesi</label>
    <input type="text" class="form-control" name="Website" style="border: groove" />
    <br />

    <label>Eğitim Derecesi</label>
    <input type="text" class="form-control" name="Degree" style="border: groove" />
    <br />

    <label>Resim</label>
    <input type="text" class="form-control" name="ImageUrl" style="border: groove" />
    <br />

    <button class="btn btn-primary">Ekle</button>
</form>
    
