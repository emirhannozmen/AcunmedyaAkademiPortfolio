
@{
    ViewBag.Title = "CreateService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Hizmet Ekleme</h2>
<form method="post">

    <label>Hizmet</label>
    <input type="text" class="form-control" name="ServiceTitle" style="border: groove" />
    <br />

    <label>Açıklama</label>
    <input type="text" class="form-control" name="Description" style="border: groove" />
    <br />

    <label>İkon</label>
    <input type="text" class="form-control" name="IconUrl" style="border: groove" />
    <br />

    <button class="btn btn-primary">Ekle</button>
</form>